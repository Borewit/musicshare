{
	"extends": [
		"tslint:recommended",
		"tslint-react",
		"tslint-config-prettier"
	],
	"rules": {
		"object-literal-shorthand": false,
		"array-type": false,
		"max-classes-per-file": false,
		"no-console": false,
		"ordered-imports": false,
		"object-literal-sort-keys": false
	},
	"linterOptions": {
		"exclude": [
			"config/**/*.js",
			"node_modules/**/*.ts",
			"coverage/lcov-report/*.js"
		]
	}
}